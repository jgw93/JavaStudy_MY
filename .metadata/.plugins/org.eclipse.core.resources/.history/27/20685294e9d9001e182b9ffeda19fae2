package ex_240304;

public class Ex_04_Class_1_Circle {

	public static void main(String[] args) {
		// 클래스 사용.
		// 클래스명 레퍼런스 변수 = new 클래스명();
		// 클래스명() : 기본생성자
		// 생성자를 호출한다.
		
		Circle koreanPizza = new Circle();
		// 클래스 필드의 접근 지정자가, public이면 보여서 접근이 가능함.
		// 접근 지정자가 private면 getter/setter로 접근이 가능한

	}

}
