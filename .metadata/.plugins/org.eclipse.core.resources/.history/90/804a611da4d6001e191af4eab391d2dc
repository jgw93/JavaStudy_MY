package ex_240229;

import java.util.Iterator;

public class Ex_3_for {

	public static void main(String[] args) {
	}
	// 입력한 수 n 까지 더하는 프로그램 만들기 " 1+2+3+...+n
	public static int sumN(int number) {
		// 초기값, 합의 값을 할당.
		int i,sum = 0;
		for(i=0; i<=number; i++) {
			sum += i;
		}
		return sum;
		
		
	}

}
